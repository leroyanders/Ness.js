{
  "name": "ness-app",
  "version": "1.2.0",
  "description": "CRA with Express.js and webpack-hot-middleware",
  "main": "server/index.mjs",
  "scripts": {
    "server": "NODE_ENV=development nodemon ./server/index.js",
    "client": "NODE_ENV=development webpack-dev-server --config webpack.config.js --mode development --hot",
    "build:css": "npx tailwindcss -i client/styles/tailwind.module.scss -o client/styles/tailwind.scss --watch",
    "build:styles": "npx tailwindcss -i client/styles/*.scss -o deploy/css/build.css --minify --watch",
    "start:ssr": "concurrently --kill-others \"yarn server\" \"yarn client\" \"yarn build:styles\"",
    "start:production": "concurrently --kill-others \"NODE_ENV=production nodemon ./server/index.js\" \"NODE_ENV=production webpack -d --config webpack.config.js --mode development --progress --watch\" \"yarn build:styles\"",
    "build": "NODE_ENV=production webpack",
    "test": "jest"
  },
  "author": "Leroy Wagner",
  "github_author": "https://github.com/leroywagner",
  "license": "MIT",
  "dependencies": {
    "@rocktimsaikia/github-card": "^3.0.1",
    "@webpack-cli/serve": "^1.7.0",
    "browser-sync": "^2.27.10",
    "browser-sync-webpack-plugin": "^2.3.0",
    "connect-livereload": "^0.6.1",
    "core-js": "^3.23.3",
    "cors": "^2.8.5",
    "css-loader": "^6.7.1",
    "css-modules-typescript-loader": "^4.0.1",
    "express": "^4.17.1",
    "he": "^1.2.0",
    "js-htmlencode": "^0.3.0",
    "livereload": "^0.9.3",
    "md5": "^2.3.0",
    "mini-css-extract-plugin": "^2.6.1",
    "node": "^12.13.0",
    "node-fetch": "^3.2.6",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^7.0.0",
    "pug": "^2.0.4",
    "pug-cli": "^1.0.0-alpha6",
    "pug-loader": "^2.4.0",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^5.0.1",
    "sass-loader": "^13.0.1",
