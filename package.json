{
  "name": "ness-app",
  "version": "3.4.2",
  "license": "MIT",
  "dependencies": {
    "@tailwindcss/postcss7-compat": "^2.2.17",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "autoprefixer": "^9.8.8",
    "axios": "^0.21.1",
    "express": "^4.18.1",
    "nessapp": "^3.6.3",
    "node-sass": "npm:sass@^1.54.4",
    "postcss": "^8.4.16",
    "postcss-loader": "^4.0.3",
    "pug": "^3.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-helmet-async": "^1.3.0",
    "react-router-dom": "^6.3.0",
    "sass": "^1.54.4",
    "shelljs": "^0.8.5"
  },
  "postcss": {
    "plugins": {
      "tailwindcss": {
        "content": [
          "./src/pages/**/*.{js,ts,jsx,tsx}",
          "./src/components/**/*.{js,ts,jsx,tsx}"
        ],
        "theme": {
          "extend": {}
        },
        "plugins": []
      },
      "autoprefixer": {}
    }
  },
  "scripts": {
    "start": "NODE_ENV=development nessapp start",
    "build": "nessapp build",
    "test": "nessapp test --env=jsdom",
    "start:prod": "NODE_ENV=production nessapp build && node deploy/server.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "postcss-preset-env": "^6.7.1",
    "sass-loader": "^7.3.1",
    "tailwindcss": "^3.1.8"
  }
